{
  "name": "warp-contracts-plugin-quickjs",
  "version": "1.1.11",
  "license": "MIT",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/types/index.d.ts",
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./build",
    "build": "yarn build:clean && yarn compile",
    "lint": "eslint --ext .js,.ts",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "require": "./build/cjs/index.js",
      "import": "./build/esm/index.js",
      "default": "./build/esm/index.js"
    }
  },
  "devDependencies": {
    "@jitl/quickjs-singlefile-mjs-release-sync": "^0.28.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.5",
    "@types/pngjs": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "arweave": "1.13.7",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "warp-arbundles": "^1.0.4",
    "warp-contracts-plugin-deploy": "^1.0.13"
  },
  "dependencies": {
    "@redstone-finance/protocol": "0.5.4",
    "pngjs": "7.0.0",
    "quickjs-emscripten": "^0.29.2",
    "seedrandom": "^3.0.5",
    "warp-contracts": "1.4.40"
  },
  "files": [
    "build/"
  ]
}
