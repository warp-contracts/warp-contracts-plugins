{
  "name": "warp-contracts-plugin-vrf",
  "version": "1.0.0-beta.1",
  "description": "A subscription plugin for warp contracts",
  "types": "./lib/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "exports": {
    ".": "./lib/cjs/index.js"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.5"
  },
  "scripts": {
    "build:cjs": "tsc -b tsconfig.json",
    "build:types": "tsc -b tsconfig.types.json",
    "build": "yarn run clean && yarn build:cjs && yarn build:types",
    "format": "prettier --write 'src/**/*.ts'",
    "clean": "rimraf ./lib",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepublishOnly": "yarn lint:fix",
    "preversion": "yarn lint && yarn build",
    "version": "yarn format && git add -A src",
    "postversion": "git push origin HEAD && git push --tags",
    "yalc:publish": "yarn build && yalc publish --push",
    "test": "jest",
    "test:unit": "jest ./src/__tests__/unit"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "README.md"
  ],
  "dependencies": {
    "@idena/vrf-js": "^1.0.1",
    "elliptic": "^6.5.4"
  },
  "peerDependencies": {
    "warp-contracts": "~1.4.3-beta.0"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "*",
    "jest": "*",
    "prettier": "*",
    "rimraf": "*",
    "ts-node": "*",
    "typescript": "*"
  }
}
